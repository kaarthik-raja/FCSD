/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
        Nand(a=a,b=a,out=NotA);
    	Nand(a=b,b=b,out=NotB);
    	Nand(a=NotA,b=b,out=O1);
    	Nand(a=NotB,b=a,out=O2);
    	Nand(a=O1,b=O2,out=out);
}